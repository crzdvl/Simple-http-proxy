{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base/legacy",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "strict": 0,
        "no-var": "error",
        "no-console": 0,
        "no-fallthrough": 0
    }
}
